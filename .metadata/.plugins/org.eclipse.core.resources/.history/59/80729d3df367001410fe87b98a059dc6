import java.math.BigInteger;
import java.util.Scanner;

class Solution {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		BigInteger n;
		n = in.nextBigInteger();
		double sum = 0;
		for (BigInteger i = 1; i <= n; i++) {
			sum = sum + Math.pow(i, i);
		}
		String sumString = "" + sum;
		// Will come with exponenents
		// Remove it
		sumString = sumString.split("E")[0];
		// Remove any decimals if present
		sumString = sumString.replaceAll("[.]", "");
		// Take last 10 character
		int length = sumString.length();
		if (length > 10)
			sumString = sumString.substring(length - 10, length);
		else
			sumString = sumString.substring(0, length);
		// Remove all leading zeroes
		while (sumString.startsWith("0")) {
			sumString = sumString.substring(1);
		}
		System.out.println(sumString);
	}
}
